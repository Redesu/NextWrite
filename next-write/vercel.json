{
    "buildCommand": "next build",
    "installCommand": "npm install",
    "routes": [
        {
            "src": "/posts/(.*)",
            "dest": "/posts/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        }
    ],
    "functions": {
        "api/**/*.md": {
            "includeFiles": "posts/**"
        }
    }
}